{
  "name": "sv-api",
  "version": "0.1.0",
  "docs": {
    "title": "SecureVote convenience API"
  },
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs": "sls downloadDocumentation --outputFileName=swagger-docs.json && ./convDocSpec.js && spectacle -t docs swagger-docs.json # bootprint openapi ./swagger-docs.json docs",
    "deploy": "npm run pkg-docker && npm run pkg-deploy && npm run docs",
    "deploy-no-docs": "npm run pkg-docker && npm run pkg-deploy",
    "pkg-deploy": "sls deploy --package pkg-docker",
    "pkg-docker": "time docker run --cpus=2 -v $(pwd):/lambda -it lambci/lambda:build-nodejs8.10 /lambda/docker-build.sh"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bn.js": "^4.11.8",
    "btoa": "^1.2.1",
    "dynamodb-doc": "^1.0.0",
    "ethers": "^3.0.26",
    "ethjs": "^0.4.0",
    "ethjs-signer": "^0.1.1",
    "io-ts": "^1.2.1",
    "ramda": "^0.25.0",
    "scrypt.js": "^0.2.0",
    "scryptsy": "^2.0.0",
    "source-map-support": "^0.5.6",
    "sv-lib": "^0.2.1",
    "web3": "^1.0.0-beta.35",
    "websocket": "^1.0.26"
  },
  "devDependencies": {
    "@types/aws-lambda": "0.0.22",
    "@types/node": "^8.10.22",
    "@types/ramda": "^0.25.35",
    "babel-runtime": "^6.26.0",
    "json-schema-to-openapi-schema": "^0.2.0",
    "serverless": "^1.29.1",
    "serverless-aws-documentation": "^1.1.0",
    "serverless-domain-manager": "^2.6.0",
    "serverless-openapi-documentation": "^0.4.0",
    "serverless-webpack": "^4.0.0",
    "ts-loader": "^2.3.7",
    "typescript": "^2.5.2",
    "webpack": "^3.12.0",
    "webpack-node-externals": "^1.7.2",
    "webpack-source-map-support": "^2.0.1",
    "yaml": "^1.0.0-rc.7"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
